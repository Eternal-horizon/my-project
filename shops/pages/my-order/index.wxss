.container{}
.order-nav{height:102rpx;background:#fff;}
.order-nav navigator{width:20%;height:102rpx;line-height:102rpx;text-align:center;color: #696969;font-size: 26rpx;}
.order-nav navigator{border-bottom:1px solid #dfdfdf;}
.order-nav text{display:inline-block;padding:0 10rpx;height:102rpx;}
.order-nav .hover text{border-bottom:1px solid #000;}
.order-head{margin-top:12rpx;}
.order-name{float:left;color: #000;font-size:26rpx;}
.order-price{float:right;color: #a1a1a1;font-size: 28rpx;}
.order-tit{float:left;color: #a1a1a1;font-size: 22rpx;}
.order-num{float:right;color: #a1a1a1;font-size: 28rpx;}
.order-specs{color: #a1a1a1;font-size: 20rpx;}
.order-title{margin: 8rpx 0 18rpx;}
.order-top{padding:26rpx 25rpx 0 0;margin-left:25rpx;padding-bottom:25rpx;border-bottom:1px solid #dfdfdf;}
.order-state view{font-size: 24rpx;color: #a1a1a1;}
.order-state text{font-size: 24rpx;color: #fd001e;}
.order-state{margin-bottom: 22rpx;}
.order-main{margin-bottom:20rpx;}
.order-payment{padding:12rpx 25rpx;color: #a1a1a1;border-bottom:1px solid #dfdfdf;}
.order-payment text{color: #000;}
.order-goods{width:162rpx;height:162rpx;margin-right:20rpx;background-repeat: no-repeat;background-position: center;background-size:cover;}
.order-title view,.order-name view{float: left;}
.order-title text,.order-name text{float: right;}
.order-name view{color:#000;font-size:26rpx;}
.order-link{}
.order-link navigator{display:inline-block;width:140rpx;height:55rpx;color:#333;line-height:55rpx;text-align:center;border:1px solid #dfdfdf;border-radius:3px;}
.order-logistics{}
.order-receipt{margin-left:18rpx;}
.order-list{background:#fff;}
.order-list:last-child .order-payment{border-bottom:0;}
.no-order{padding-top:162rpx;text-align: center;}
.no-order view{margin-bottom:24rpx;font-size: 26rpx;color: #a1a1a1;}
.no-order navigator{text-decoration:underline;font-size: 26rpx;color: #000;}